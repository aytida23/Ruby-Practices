#the script create an array of different fruits and store them in an array and
#Loop through each of the fruits and display to the screen "Om nom nom [FRUIT NAME]"

fruits = []

fruits << "Apples"
fruits << "Oranges"
fruits << "Strawberries"
fruits << "Mangoes"

fruits.each do |new|
  upcase_fruits_name = new.upcase
  puts "Om nom nom [#{upcase_fruits_name}]"
end
